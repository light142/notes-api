{
    "builds": [
        {
            "src": "notes_project/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "notes_project/wsgi.py",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, OPTIONS, DELETE, PUT"
            }
        }
    ]
}